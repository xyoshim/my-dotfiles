[core]
	autocrlf = false
	filemode = true
	safecrlf = warn
	editor = "/usr/bin/env" "vim"
	pager = "/usr/bin/env" "less" "-FRX"
[user]
	name = MVA
	email = mva000@gmail.com
[alias]
	graphlog = log --pretty='format:%C(auto)%h %as%d %s' --date=local --graph
	changelog = log --pretty='format:%C(auto)%as  %an <%ae>%n    %s%n' --date=local
	dlg = log --date=local --graph
[color]
	ui = auto
