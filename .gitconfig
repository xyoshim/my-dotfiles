[core]
	autocrlf = false
	filemode = false
	safecrlf = warn
	editor = "/usr/bin/env" "vim"
	pager = "/usr/bin/env" "less" "-FRX"
[user]
	name = MVA
	email = mva000@gmail.com
[alias]
	graphlog = log --pretty='format:%C(auto)%h %as%d %s' --date=local --graph
	changelog = log --pretty='format:%C(auto)%as  %an <%ae>%n  * %s%n' --date=local
	dlg = log --date=local --graph
	sha1-short7 = rev-parse --short=7
	sha1-short7-head = rev-parse --short=7 HEAD
[safe]
	directory = %(prefix)///alma9/opt/src/git-repo/xz
[gui]
	recentrepo = H:/src/git/pcre2
[color]
	ui = auto
[bash]
	showDirtyState = 1
	showUpstream = auto
